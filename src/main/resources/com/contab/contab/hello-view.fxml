<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.contab.contab.HelloController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
         <top>
            <MenuBar prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="Ventas">
                  <items>
                    <MenuItem fx:id="ventasForm" mnemonicParsing="false" onAction="#ventasFor" text="ventas form" />
                        <MenuItem fx:id="cobrodeventas" mnemonicParsing="false" text="cobro de ventas " />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Compras">
                  <items>
                    <MenuItem mnemonicParsing="false" text="compras form" />
                        <MenuItem mnemonicParsing="false" text="pago de ventas " />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
         
                   <Label fx:id="welcomeText" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
